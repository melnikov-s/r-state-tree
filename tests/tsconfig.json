{
	"compilerOptions": {
		"rootDir": "../",
		"target": "es2022",
		"alwaysStrict": false,
		"noImplicitReturns": false,
		"noUnusedLocals": false,
		"noImplicitAny": false,
		"noImplicitThis": false,
		"lib": ["es2022"],
		"types": ["vitest/globals"],
		"strictNullChecks": false,
		"downlevelIteration": true,
		"esModuleInterop": true,
		"moduleResolution": "node"
	},
	"include": ["*"],
	"exclude": ["node_modules"]
}
